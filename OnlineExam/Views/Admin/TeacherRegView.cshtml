@model OnlineExam.DbContext.Teachers_Registration

@{
    ViewBag.Title = "TeacherRegView";
}




<section class="pt-5 bg-section-secondary">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
