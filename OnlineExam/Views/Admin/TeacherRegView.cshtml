                                    <dt class="col-sm-3 h6 text-sm">Phone</dt>
                                    <dd class="col-sm-9 text-sm">+30-588.90.86</dd>
                                    <dt class="col-sm-3 h6 text-sm">Street</dt>
                                    <dd class="col-sm-9 text-sm">8945 Henry Ave. Mankato, MN 56001</dd>
                                </dl>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-4 justify-content-center text-center">
                    <div class="col-lg-7 col-md-9 mb-5">
                        <button type="button" class="btn btn-sm btn-primary">Print Application</button>
                        <a href="TeacherRegistrations" class="btn btn-sm btn-secondary text-dark font-weight-bold">Back To List</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


